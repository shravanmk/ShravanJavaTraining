package oop;

public class AccountApp {

	public static void main(String[] args) {
         
		LoanAccount la = new LoanAccount();
		la.setRate();
		la.IncreaseRate();
		la.setTerm(20);
		la.ammsched();

		//Polymorphism changes where we are pointing
		IRate account1 = new LoanAccount();
		account1.setRate();
		account1.IncreaseRate();
				
	}

}
